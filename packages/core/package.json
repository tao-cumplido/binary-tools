{
	"name": "@binary-tools/core",
	"version": "0.10.0-alpha.8",
	"description": "Supercharged data view for reading binary data in JavaScript from an async data source",
	"keywords": [
		"binary",
		"hex",
		"data",
		"view",
		"dataview",
		"javascript",
		"typescript"
	],
	"license": "ISC",
	"author": "Tao Cumplido",
	"repository": "https://github.com/tao-cumplido/binary-tools/tree/main/packages/core",
	"bugs": "https://github.com/tao-cumplido/binary-tools/issues",
	"type": "module",
	"imports": {
		"#*.ts": {
			"types": "./src/*.ts",
			"test": "./src/*.ts",
			"default": "./dist/*.js"
		}
	},
	"exports": {
		".": "./dist/entrypoints/index.js",
		"./decoder": "./dist/entrypoints/decoder.js",
		"./encoder": "./dist/entrypoints/encoder.js",
		"./patterns": "./dist/entrypoints/patterns.js"
	},
	"scripts": {
		"build": "wireit",
		"test": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc",
			"clean": "if-file-deleted",
			"files": [
				"src/**/*.ts",
				"!src/**/*.test.ts",
				"tsconfig.json"
			],
			"output": [
				"dist"
			]
		},
		"test": {
			"command": "node --conditions test --test-reporter @voxpelli/node-test-pretty-reporter --test 'src/**/*.suite.test.ts'",
			"files": [
				"src/**/*.test.ts",
				"tsconfig.test.json"
			]
		}
	},
	"dependencies": {
		"@shigen/enum": "~0.5.0",
		"type-fest": "^4.41.0",
		"ts-pattern": "^5.8.0",
		"regex": "^6.0.1"
	}
}
